{
	"jenkinsUrl": "http://localhost:8080/",
	"checkInterval": 5000,
	"maximumRetries": -1,
	"stopAfterOnePositiveCheck": true,
	"onlyExecuteCommandOnChange": true,
	"startupCommand": "startup command",
	"shutdownCommand": "",
	"watchedJobs": [
	  "Test"
	],
	"checks": [
		{
			"id": "Building",
			"scope": "one",
			"status": ["red_anime", "blue_anime", "notbuilt_anime", "aborted_anime", "aborted"],
			"command": "building command"
		},
		{
			"id": "One Build Failed",
			"scope": "one",
			"status": ["red"],
			"command": "failed command"
		},
		{
			"id": "All Builds Ok",
			"scope": "all",
			"status": ["blue", "notbuilt", "aborted"],
			"command": "ok command"
		}
	]
}
